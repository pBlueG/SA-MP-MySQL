{
  "defaultBrowser": "Google Chrome",
  "rewrite": [
    {
      "match": {
        "protocol": "http"
      },
      "url": {
        "protocol": "https"
      }
    },
    {
      "match": "**/*example.org/*",
      "url": "http://example.com"
    }
  ],
  "handlers": [
    {
      "match": [
        "**/*apple.com/*",
        "**/*example.org/*",
        "**/*example.com**"
      ],
      "browser": "Safari"
    },
    {
      "match": "/workplace/",
      "browser": "Firefox"
    },
    {
      "match": [
        "**/*google.com/*",
        "**/*.google.com/*"
      ],
      "browser": "Google Chrome"
    }
  ]
}